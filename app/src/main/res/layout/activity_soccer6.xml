        android:layout_height="40dp"
        app:srcCompat="@drawable/defender"
        android:layout_alignTop="@+id/iv7_player7"
        android:layout_toEndOf="@+id/iv7_player5" />

    <ImageView
        android:id="@+id/iv7_player10"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/iv7_player5"
        android:layout_marginStart="11dp"
        android:layout_toEndOf="@+id/iv7_player9"
        app:srcCompat="@drawable/defender" />

    <ImageView
        android:id="@+id/iv7_player11"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/iv7_player8"
        android:layout_below="@+id/iv7_player8"
        android:layout_marginTop="42dp"
        app:srcCompat="@drawable/goalkeeper" />

    <TextView
        android:id="@+id/tv7_player1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player1"
        android:layout_below="@+id/iv7_player1"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:layout_alignEnd="@+id/iv7_player1" />

    <TextView
        android:id="@+id/tv7_player2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv7_player2"
        android:layout_toStartOf="@+id/iv7_player10"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:text="Player2" />

    <TextView
        android:id="@+id/tv7_player3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv7_player3"
        android:layout_centerHorizontal="true"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:text="Player3" />

    <TextView
        android:id="@+id/tv7_player4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:layout_below="@+id/iv7_player4"
        android:layout_toEndOf="@+id/iv7_player7"
