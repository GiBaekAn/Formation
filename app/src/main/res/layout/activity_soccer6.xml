    <TextView
        android:id="@+id/tv7_player8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:layout_below="@+id/iv7_player8"
        android:layout_centerHorizontal="true"
        android:text="Player8" />

    <TextView
        android:id="@+id/tv7_player9"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv7_player2"
        android:layout_below="@+id/iv7_player9"
        android:text="Player9" />

    <TextView
        android:id="@+id/tv7_player10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player10"
        android:layout_below="@+id/iv7_player10"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
