        android:layout_alignBottom="@+id/tv7_player4"
        android:layout_alignEnd="@+id/iv7_player5"
        android:text="Player5" />

    <TextView
        android:id="@+id/tv7_player6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        android:layout_below="@+id/iv7_player6"
        android:layout_toStartOf="@+id/tv7_player1"
